@model IEnumerable<ProgramPlanner.Models.TrimesterCourse>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Course.CourseName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Trimester.TrimesterID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Year)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Course.CourseName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Trimester.TrimesterID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Year)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { TrimesterID = item.TrimesterID, CourseID = item.CourseID }) |
            @Html.ActionLink("Details", "Details", new { TrimesterID = item.TrimesterID, CourseID = item.CourseID }) |
            @Html.ActionLink("Delete", "Delete", new { TrimesterID = item.TrimesterID, CourseID = item.CourseID })
        </td>
    </tr>
}

</table>
