@model IEnumerable<ProgramPlanner.Models.Test>

@{
   
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                Replaced
            </th>
            <th>
                Replacement
            </th>
            <th>
                Year Degree
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <th>
                @item.Replaced.CourseCode
            </th>
            <td>
                @item.Replacement.ReplacementCourse.CourseCode
            </td>
            <td>
                @item.YearDegree.Year
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Replacement.ReplacementID }) |
                @Html.ActionLink("Details", "Details", new { id=item.Replacement.ReplacementID }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.Replacement.ReplacementID })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
